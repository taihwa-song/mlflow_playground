name: HyperparameterSearch

conda_env: conda.yaml

entry_points:
  train:
    parameters:
      max_depth: {type: int, default: 10}
      min_samples_leaf: {type: int, default: 1}
    command: "python train.py
                            --max-depth {max_depth}
                            --min-samples-leaf {min_samples_leaf}"